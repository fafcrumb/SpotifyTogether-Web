{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-queue.html",
    "src/my-view3.html",
    "src/my-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [{
    "name": "default",
    "preset": "es6-unbundled",
    "js": {
      "minify": false
    },
    "swPrecacheConfig": "sw-precache-config.js"
  }],
  "lint": {
    "rules": ["polymer-2"]
  }
}
